#FancyUrl Stuff
Options +FollowSymLinks
RewriteEngine On

#PageHandler
RewriteRule (Page/)(.{0,}) index.php?page=$2
RewriteRule (page/)(.{0,}) index.php?page=$2

#Users cant open dir Classes
RewriteRule (Classes).{0,} index.php?page=404

#Users cant open dir ViewCounter
RewriteRule (Design).{0,} index.php?page=404

#Users cant open dir Disign
RewriteRule (ViewCounter).{0,} index.php?page=404

#Pictures are not in pictures
RewriteRule (Pictures)(.{0,}) Content/Pictures$2

#cant open htaccess files
<files .htaccess>
order allow,deny
deny from all
</files>